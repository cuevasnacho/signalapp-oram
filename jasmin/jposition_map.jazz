require "position_map.jinc"

export
fn scan_position_map_get_jazz(
  reg u64 scan_position_map,
  reg u64 block_id,
  reg u64 position
)
{
  scan_position_map_get(scan_position_map, block_id, position);
}

export
fn scan_position_map_set_jazz(
  reg u64 scan_position_map,
  reg u64 block_id position,
  reg u64 prev_position
)
{
  scan_position_map_set(scan_position_map, block_id, position, prev_position);
}
